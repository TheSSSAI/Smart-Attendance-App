# .gitignore for Flutter projects

# General
.DS_Store
.idea/
.vscode/
build/
.dart_tool/

# Flutter specific
.flutter-plugins
.flutter-plugins-dependencies
.packages
.pub-cache/
.pub/

# IDE specific files
*.iml
*.ipr
*.iws

# Build artifacts
/build/
/ios/build/
/android/build/
/web/build/

# Generated files
*.g.dart
*.freezed.dart

# Test reports and coverage
coverage/
test/coverage/

# Sensitive environment files
*.env

# Flutter/Dart logs
.log

# Windows specific
Thumbs.db
Desktop.ini

# Android files
/android/app/src/main/assets/flutter_assets/
/android/key.properties
/android/local.properties
/android/.gradle/
/android/gradle/
/android/gradlew
/android/gradlew.bat

# iOS files
/ios/Pods/
/ios/Podfile.lock
/ios/.symlinks/
/ios/Flutter/App.framework
/ios/Flutter/engine
/ios/Flutter/flutter_assets
/ios/Flutter/Generated.xcconfig
/ios/Flutter/info.plist
/ios/Flutter/flutter_export_environment.sh
/ios/Runner/GeneratedPluginRegistrant.*
/ios/Runner.xcworkspace/
/ios/Runner.xcodeproj/project.xcworkspace/
/ios/Runner.xcodeproj/xcuserdata/

# macOS files
/macos/Flutter/ephemeral
/macos/Pods/
/macos/Podfile.lock
/macos/.symlinks/
/macos/Runner.xcworkspace/
/macos/Runner.xcodeproj/project.xcworkspace/
/macos/Runner.xcodeproj/xcuserdata/

# Web files
/web/
!/web/index.html
!/web/favicon.png
!/web/icons/